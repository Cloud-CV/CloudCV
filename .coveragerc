[run]
omit =
    */python?.?/*
    */migrations/*
source = cloudcv/

[report]
exclude_lines =
    import
    @api_view
    @authentication_classes
    @permission_classes
    @csrf_exempt

omit =
    */scripts/*
    */docker/*
    */aws/*
    */templates/*
    */static/*
    */python?.?/*
    */migrations/*
    */tests/*
    */middleware/*
    frontend/
    settings/prod/*
    settings/staging/*
    settings/dev/*
