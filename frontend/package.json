{
  "name": "cloudCV",
  "version": "2.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node server/buildHTML.js",
    "build": "npm-run-all clean-dist test lint:build build:html && babel-node server/build.js",
    "build:alias": "babel-node server/build.js",
    "start": "npm-run-all build && node server/start.js",
    "open:src": "babel-node server/srcServer.js",
    "lint": "esw webpack.config.* src server",
    "lint:build": "node_modules/.bin/esw webpack.config.* src server -c ./.eslintrc-build",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "eslint src server webpack.config.* --fix",
    "test": "mocha --reporter spec server/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "dev": "node_modules/.bin/babel-node server/startMessage.js && npm-run-all --parallel open:src lint:watch test:watch"
  },
  "author": "Utkarsh Gupta",
  "license": "GPL-3.0",
  "dependencies": {
    "antd": "^2.10.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1",
    "superagent": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.16.3",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "4.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^7.0.0",
    "eslint-watch": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.11.1",
    "jsdom": "^10.1.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "mocha": "^3.1.0",
    "nock": "^9.0.11",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "open": "^0.0.5",
    "prettier": "^1.3.1",
    "react-addons-test-utils": "^15.3.2",
